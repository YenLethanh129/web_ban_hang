{
  "Serilog": {
    "Using": [
      "${Serilog__Using__0}",
      "${Serilog__Using__1}"
    ],
    "MinimumLevel": {
      "Default": "${Serilog__MinimumLevel__Default}",
      "Override": {
        "Microsoft": "${Serilog__MinimumLevel__Override__Microsoft}",
        "System": "${Serilog__MinimumLevel__Override__System}"
      }
    },
    "WriteTo": [
      {
        "Name": "${Serilog__WriteTo__0__Name}"
      },
      {
        "Name": "${Serilog__WriteTo__1__Name}",
        "Args": {
          "path": "${Serilog__WriteTo__1__Args__path}",
          "rollingInterval": "${Serilog__WriteTo__1__Args__rollingInterval}",
          "rollOnFileSizeLimit": "${Serilog__WriteTo__1__Args__rollOnFileSizeLimit}",
          "shared": "${Serilog__WriteTo__1__Args__shared}",
          "retainedFileCountLimit": "${Serilog__WriteTo__1__Args__retainedFileCountLimit}",
          "fileSizeLimitBytes": "${Serilog__WriteTo__1__Args__fileSizeLimitBytes}"
        }
      }
    ]
  },

  "Security": {
    "PasswordSalt": "${Security__PasswordSalt}",
    "EncryptionKey": "${Security__EncryptionKey}",
    "EncryptionIV": "${Security__EncryptionIV}",
    "Jwt": {
      "Issuer": "${Security__Jwt__Issuer}",
      "Audience": "${Security__Jwt__Audience}",
      "SecretKey": "${Security__Jwt__SecretKey}",
      "ExpirationMinutes": "${Security__Jwt__ExpirationMinutes}"
    }
  },

  "AllowedHosts": "${AllowedHosts}",

  "ConnectionStrings": {
    "WebbanhangDB": "${ConnectionStrings__WebbanhangDB}"
  },

  "Encryption": {
    "Key": "${Encryption__Key}"
  },

  "ImageUpload": {
    "StorageType": "${ImageUpload__StorageType}",
    "S3": {
      "BucketName": "${ImageUpload__S3__BucketName}",
      "Region": "${ImageUpload__S3__Region}",
      "AccessKey": "${ImageUpload__S3__AccessKey}",
      "SecretKey": "${ImageUpload__S3__SecretKey}",
      "CloudFrontDomain": "${ImageUpload__S3__CloudFrontDomain}"
    }
  }
}
